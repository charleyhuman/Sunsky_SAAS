<body>

<div class="container">          
  <table class="table table-hover sortable">
    <thead>
      <tr>
        <th>Class Name</th>
        <th>Class Number</th>
        <th>CE Hours</th>
        <th>Grade</th>
      </tr>
    </thead>
    <tbody>
      <% @user_classes.each do |x| %>
          <tr>
              <td><%= @class_names[x.class_number] %></td>
              <td><%= x.class_number %></td>
              <td><%= x.ce_hours %></td>
              <td><%= x.grade %></td>
          </tr>
      <% end %>
    </tbody>
  </table>
</div>


</body>